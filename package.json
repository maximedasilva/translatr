{
  "name": "translatr",
  "displayName": "translatr",
  "description": "take your textkey and find show your translations",
  "version": "2.3.0",
  "author": {
    "name": "Maxime Da Silva",
    "url": "https://github.com/maximedasilva"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maximedasilva/translatr"
  },
  "icon": "./public/Logo.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:./.vscode/translatr.config.js"
  ],
  "extensionKind": [
    "ui"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true,
      "description": ""
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "contributes": {
    "commands": [
      {
        "command": "translatr.reloadTranslations",
        "title": "Reload the translations"
      },
      {
        "command": "translatr.reloadTranslatrconfig",
        "title": "Reload the translatr config"
      }
    ]
  },
  "devDependencies": {
    "@types/babel__core": "7.20.5",
    "@types/esm": "3.2.2",
    "@types/esprima": "4.0.6",
    "@types/estraverse": "5.1.7",
    "@types/mocha": "10.0.10",
    "@types/node": "24.x",
    "@types/vscode": "1.70.0",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "@vscode/test-cli": "0.0.12",
    "@vscode/test-electron": "2.5.2",
    "typescript": "5.9.3",
    "webpack": "5.103.0",
    "webpack-cli": "6.0.1"
  },
  "dependencies": {
    "@babel/core": "7.28.5",
    "@babel/parser": "7.28.5",
    "@babel/plugin-transform-modules-commonjs": "7.27.1",
    "@babel/preset-typescript": "7.28.5",
    "babel-loader": "10.0.0",
    "eslint": "9.39.1",
    "ts-loader": "9.5.4"
  }
}
